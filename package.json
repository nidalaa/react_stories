{
  "name": "react_stories",
  "description": "Just a training app in react",
  "version": "0.0.0",
  "engines": {
    "node": "0.10.x"
  },
  "main": "src/app.js",
  "scripts": {
    "start": "http-server dist --silent",
    "watch": "npm start & watchify . -o dist/app.js --verbose --debug --extension .jsx",
    "postinstall": "browserify . -o dist/app.js --verbose --debug --extension .jsx"
  },
  "author": "Emilia Andrzejewska",
  "license": "BSD-2-Clause",
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "devDependencies": {
    "watchify": "~2.4.0"
  },
  "dependencies": {
    "http-server": "~0.7.5",
    "browserify": "~9.0.3",
    "reactify": "~1.0.0",
    "react": "~0.12.2",
    "jquery": "~2.1.3",
    "react-bootstrap": "~0.16.1",
    "bootstrap": "~3.3.2",
    "react-router": "~0.12.4"
  }
}
